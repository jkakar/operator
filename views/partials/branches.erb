<ul class="list-unstyled deploy-list">
  <% branches.each do |branch| %>
    <li>
      <h4>
        <a href="<%= deploy_path(branch: branch.name) %>" class="btn btn-primary btn-xs btn-success deploy-button">Ship This!</a>
        &nbsp;
        <%= deploy_type_icon("branch") %> <%= branch.name %>
        &nbsp;
        <a href="<%= github_branch_url(branch) %>" class="text-muted" title="view on github"><i class="icon-github-sign"></i></a>
      </h4>
      <small class="muted">commit: <a href="<%= github_commit_url(branch.commit) %>" title="<%= branch.commit.sha %>"><%= sha_span(branch.commit.sha) %></a></small>
      <hr>
    </li>
  <% end %>
</ul>
