#!/usr/bin/env bash
set -euo pipefail

echo "receive-results from runner id '${PRIVET_RUNNER_ID}'"
echo "  units run: $(echo -n "$PRIVET_UNITS" | tr "\n" ",")"
echo "  unit results:"
echo "    exit code: ${PRIVET_UNIT_RESULT_CODE}"
echo "    output: $(cat "$PRIVET_UNIT_RESULT_FILE")"
echo
echo "  additional results:"
echo "    exit code: ${PRIVET_ADDITIONAL_RESULT_CODE}"
echo "    output: $(cat "$PRIVET_ADDITIONAL_RESULT_FILE")"
echo